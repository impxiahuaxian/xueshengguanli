<script type="text/ecmascript">
    function Edit(id) {
        if (confirm("确定删除操作吗？")) {
                $.ajax({
                    type: "post",
                    traditional: true,
                    url: "/Goods/Del",
                    data: { ID: id },
                    datatpye: "json",
                    success: function (data) {
                        if (data) {
                           alert("操作成功！")
                                window.location.reload();
                        }
                    }
                });
            }
    }
</script>