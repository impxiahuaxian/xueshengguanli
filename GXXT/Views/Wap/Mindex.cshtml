@model Maticsoft.Model.Users
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, Users-scalable=no">
    <title>个人中心</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <script src="/Mobile/js/rem.js"></script> 
    <script src="/Mobile/js/jquery.min.js" type="text/javascript"></script>
    <link rel="stylesheet" type="text/css" href="/Mobile/css/base.css">
    <link rel="stylesheet" type="text/css" href="/Mobile/css/page.css">
    <link rel="stylesheet" type="text/css" href="/Mobile/css/all.css">
    <link rel="stylesheet" type="text/css" href="/Mobile/css/mui.min.css">
    <link rel="stylesheet" type="text/css" href="/Mobile/css/loaders.min.css">
    <link rel="stylesheet" type="text/css" href="/Mobile/css/loading.css">
    <link rel="stylesheet" type="text/css" href="/Mobile/slick/slick.css">
      <link href="../../Mobile/css/demo.css" rel="stylesheet" />
    <link href="../../Mobile/css/iconfont.css" rel="stylesheet" />
	
</head>
	<body>
		<div class="center-header clearfloat" id="header">
			
			<div class="top fl clearfloat box-s">
				<a href="#">
					<div class="tu fl">
						<span></span>
						<img src="/Mobile/images/touxiang.png">
					</div>
					<p class="tit fl">@Model.Name</p> 
                
                   
			</div>
			<div class="bottom clearfloat">
				<ul>
                    <li>
						<a href="/Wap/index" class="clearfloat">
							
							<p>首页</p>
						</a>
					</li>
					<li>
						<a href="#" class="clearfloat">
							
							<p>@Model.UserName</p>
						</a>
					</li>
					<li>
						<a href="/Home/mhyxx" class="clearfloat">
							
							<p>押金：@Model.Deposit </p>
						</a>
					</li>
					<li>
						<a href="/Home/mhyxx" class="clearfloat">
							
							<p>余额：@Model.Balance </p>
						</a>
					</li>
				</ul>
			</div>
		</div>
		
		<div class="clearfloat pcenter" id="main">

			<div class="p-list p-listwo clearfloat box-s">
				<a href="/Wap/Create" class="clearfloat">
					 <i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/fb.png" /></i>
					<span class="fl" >发布物品</span>
					
				</a>
			</div>
            <div class="p-list p-listhree clearfloat box-s">
				<a href="/Wap/mmanage" class="clearfloat">
					 <i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/fb.png" /></i>
					<span class="fl" >我的物品</span>
				</a>
			</div>
		
			
			
			<div class="p-list p-listwo clearfloat box-s">
				<a href="/Wap/omanage?PUserID=@Maticsoft.BLL.Users.GetNowUserID()" class="clearfloat">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/re.png" /></i>
					<span class="fl" >我租的订单</span>
				</a>
			</div>
            <div class="p-list p-listwo clearfloat box-s">
				<a href="/Wap/omanage?UserID=@Maticsoft.BLL.Users.GetNowUserID()" class="clearfloat">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/re.png" /></i>
					<span class="fl" >我出租的订单</span>
				</a>
			</div>
             <div class="p-list p-listwo clearfloat box-s">
				<a href="/Wap/emanage" class="clearfloat">
						<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/re.png" /></i>
					<span class="fl" >我的收藏</span>
				
				</a>
			</div>

			<div class="p-list p-listhree clearfloat box-s">
				<a href="/Wap/Gywm" class="clearfloat">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/about.png" /></i>
					<span class="fl">关于我们</span>
				</a>
			</div>
			<div class="p-list p-listhree clearfloat box-s">
				<a href="/Wap/medit" class="clearfloat">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/xg.png" /></i>
					<span class="fl">修改个人信息</span>
					
				</a>
			</div>
            <div class="p-list p-listhree clearfloat box-s">
				<a href="/Wap/CZ" class="clearfloat">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/xg.png" /></i>
					<span class="fl">我要充值</span>
					
				</a>
			</div>
            <div class="p-list p-listhree clearfloat box-s">
				<a href="/Wap/TX" class="clearfloat">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/xg.png" /></i>
					<span class="fl">我要提现</span>
					
				</a>
			</div>
            <div class="p-list p-listhree clearfloat box-s">
				<a href="#" class="clearfloat" id="btnQiut">
					<i class="iconfont icon-favorite fl xing"><img src="/Mobile/images/tc.png" /></i>
					<span class="fl">退出登录</span>
				</a>
			</div>
		</div>
		
	
	</body>
	<script type="text/javascript" src="/Mobile/js/jquery-1.8.3.min.js" ></script>
	<script type="text/javascript" src="/Mobile/slick/slick.min.js" ></script>
	<script type="text/javascript" src="/Mobile/js/jquery.leanModal.min.js"></script>
	<script type="text/javascript" src="/Mobile/js/tchuang.js" ></script>
	<script type="text/javascript" src="/Mobile/js/hmt.js" ></script>
</html>
<script type="text/ecmascript">
    $(function () {
        $("#btnQiut").click(function () {
            $.ajax({
                type: "post",
                url: "/Home/LogOff",
                success: function (data) {
                    if (data) {
                        parent.location.href = "/Wap/mindex";
                    }
                }
            });
        });
    });
</script>
